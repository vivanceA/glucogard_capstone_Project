{
  "expo": {
    "name": "gluco-gard",
    "slug":"glucogard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      },
      "EXPO_PUBLIC_WEB_DASHBOARD_URL": "https://localhost:8081/web-dashboard",
      "EXPO_PUBLIC_RESEARCH_PORTAL_URL": "https://localhost:8081/research-portal"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
